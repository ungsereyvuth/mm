            </div>
          </div>
        </div>
        <a href="shop-grid.html" class="btn btn btn-light shadow-sm sa-btn">See All <i data-feather="arrow-right"></i></a>
      </div>
      <!-- /Men's Fashion -->

      <hr>

      <div class="row gutters-3">

        <!-- Men's Fashion -->
        <div class="col-6 d-flex">
          <div class="card card-2col flex-fill">
            <div class="d-flex flex-column-reverse flex-md-row">
              <div class="card-2col-body">
                <h2 class="card-title">Men's Fashion</h2>
                <div class="list-group list-group-flush list-group-no-border list-group-sm w-100 text-center">
                  <a href="shop-grid.html" class="list-group-item list-group-item-action">Clothing</a>
                  <a href="shop-grid.html" class="list-group-item list-group-item-action">Shoes</a>
                  <a href="shop-grid.html" class="list-group-item list-group-item-action">Watches</a>
                  <a href="shop-grid.html" class="list-group-item list-group-item-action">Accessories</a>
                </div>
                <a href="shop-grid.html" class="btn btn-primary rounded-pill mt-3" data-addclass-on-xs="btn-sm">Shop Now</a>
              </div>
              <div class="card-2col-img">
                <div data-cover="../img/categories/men.jpg" data-xs-height="120px" data-sm-height="200px" data-md-height="236px" data-lg-height="244px" data-xl-height="250px"></div>
              </div>
            </div>
          </div>
        </div>
        <!-- /Men's Fashion -->

        <!-- Women's Fashion -->
        <div class="col-6 d-flex">
          <div class="card card-2col flex-fill">
