                <a href="shop-grid.html" class="btn btn-danger rounded-pill mt-3" data-addclass-on-xs="btn-sm">Shop Now</a>
              </div>
              <div class="card-2col-img">
                <div data-cover="../img/categories/women.jpg" data-xs-height="120px" data-sm-height="200px" data-md-height="236px" data-lg-height="244px" data-xl-height="250px"></div>
              </div>
            </div>
          </div>
