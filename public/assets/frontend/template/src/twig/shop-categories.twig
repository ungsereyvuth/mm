<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    {% include '_required-css.twig' %}

    {% include '_template-css.twig' %}

    <title>Shop Categories - {{title}}</title>
  </head>
  <body>

    {% include '_header.twig' with {'active':'shop', 'sub':'shop-categories'} %}

    <!-- Main Content -->
    <div class="container my-3">

      <!-- Men's Fashion -->
      <div class="row categories-grid gutters-3">
        <div class="col-sm-6 col-md-4">
          <a href="shop-grid.html">
            <div class="card zoom-hover">
              <div data-cover="../img/categories/1.jpeg" data-xs-height="150px" data-sm-height="272px" data-md-height="246px" data-lg-height="324px" data-xl-height="400px"></div>
              <div class="card-img-overlay card-img-overlay-transparent">
                <h2 class="card-title bold">Men's Fashion</h2>
              </div>
            </div>
          </a>
        </div>
        <div class="col-sm-6 col-md-4">
          <div class="row gutters-3">
            <div class="col-6 col-sm-12 mt-3 mt-sm-0">
              <a href="shop-grid.html">
